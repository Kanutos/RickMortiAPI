# Ignore node_modules directory
node_modules/

# Ignore .env file
.env

# Ignore any files generated by the operating system
.DS_Store
Thumbs.db

# Ignore logs and other files generated by your editor
logs/
*.log

# Ignore npm debug log
npm-debug.log*

# Ignore files specific to Visual Studio Code
.vscode/

# Ignore IDE settings files
.idea/
*.iml

# Ignore compiled files
*.class
*.jar
*.war
*.ear
*.hprof
*.min.js

# Ignore files related to testing
coverage/

# Ignore environment-specific files
.env.local
.env.development.local
.env.test.local
.env.production.local
.env*.local
.env.*.local
